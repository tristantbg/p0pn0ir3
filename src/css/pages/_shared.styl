[event-target]
	cursor pointer

#artists-preview
	position fixed
	top 0
	left 0
	width 100%
	height 100%
	z-index 5
	pointer-events none
	.artist-preview
		position absolute
		top 0
		left 0
		width 100%
		height 100%
		visibility hidden
		.responsive-image
			width 100%
			height 100%
			img
				width 100%
				height 100%
				object-fit cover
				font-family 'object-fit: cover'